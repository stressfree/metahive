<div class="yui-g" xmlns:spring="http://www.springframework.org/tags" xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0">
  <jsp:directive.page contentType="text/html;charset=UTF-8" />
  <jsp:output omit-xml-declaration="yes" />
  <spring:message var="title" code="metahive_welcome_title" htmlEscape="true" />
  <util:panel id="title" title="${title}">
    <p>
      <spring:message code="metahive_welcome_message" htmlEscape="false" />
    </p>
  </util:panel>
</div>